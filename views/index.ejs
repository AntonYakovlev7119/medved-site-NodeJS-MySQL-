<%-include("./__blocks/__header.ejs") %>

<main class="page-content">
  <section class="intro">
    <div class="intro-wrapper">
      <div class="intro__background">
        <div class="intro__background-img"></div>
      </div>
      <div class="intro__left-side">
        <div class="intro__logo">
          <img src="./img/logo3.png" alt="logo" />
          <span class="intro__company-name">ООО<br />&laquoМедведь&raquo</span>
        </div>
        <div class="intro__left-side-scew">
          <svg
            xmlns="http://www.w3.org/2000/svg"
            xmlns:svg="http://www.w3.org/2000/svg"
            width="100%"
            height="100%"
            preserveAspectRatio="none"
            viewBox="0 0 210 297"
            version="1.1"
          >
            <path d="M 0, 0 V 300 L 200, 0 Z" />
          </svg>
        </div>
      </div>
      <div class="intro__right-side">
        <div class="intro__site-description">
          <span> <%= content.header_desc %> </span>
          <a href="#wood"><span></span></a>
        </div>
      </div>

      <div class="intro__bottom-gradient"></div>
    </div>
  </section>

